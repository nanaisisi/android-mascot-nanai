{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "mascot_nanai",
	"version": "0.1.0",
	"identifier": "com.mascot-nanai.app",
	"build": {
		"frontendDist": "../src"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"title": "main",
				"width": 800,
				"height": 600,
				"transparent": true,
				"decorations": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"active": false
		}
	}
}
